{
  "name": "smart-waste-management",
  "version": "1.0.0",
  "description": "A complete web application for smart waste management with location-based bin tracking",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "demo": "node demo-server.js",
    "seed": "node seed.js",
    "setup": "npm install && npm run seed",
    "build": "echo 'Backend ready for deployment'",
    "build:frontend": "cp -r frontend/* dist/ || xcopy /E /I frontend dist",
    "serve": "cd frontend && python -m http.server 8080 || cd frontend && npx serve .",
    "deploy": "npm run build && echo 'Ready for deployment'",
    "railway:start": "npm run seed && npm start",
    "railway:deploy": "node deploy-railway.js deploy",
    "railway:clean": "node deploy-railway.js clean",
    "railway:check": "node deploy-railway.js check",
    "railway:logs": "node deploy-railway.js logs",
    "railway:env": "node deploy-railway.js env",
    "test:db": "node test-db-connection.js",
    "postinstall": "echo 'Installation complete'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "waste-management",
    "nodejs",
    "express",
    "mongodb",
    "geolocation",
    "maps"
  ],
  "author": "Smart Waste Management Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  }
}
